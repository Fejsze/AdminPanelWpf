<Window
    x:Class="LearningApp.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:LearningApp"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="C#-ot C#-ul"
    WindowState="Maximized"
    VerticalAlignment="Stretch" HorizontalAlignment="Stretch"
    ResizeMode="NoResize"
    Background="#008AFF"
    mc:Ignorable="d" Icon="Resource/32px-C_Sharp_wordmark.svg.png">
    <StackPanel Margin="0,0,-0.4,0">
        <Frame Name="frame" Content="{Binding Path=DisplayPage, Mode=TwoWay}" NavigationUIVisibility="Hidden"/>
    </StackPanel>
</Window>
